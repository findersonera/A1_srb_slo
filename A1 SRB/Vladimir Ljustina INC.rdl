  <DataSets>
    <DataSet Name="a1telekom_prd_ar">
      <Query>
        <DataSourceName>a1telekom_prd_ar</DataSourceName>
        <CommandText>SELECT
  HPD_HELP_DESK.Incident_Number ,
  HPD_HELP_DESK.Description,
  HPD_HELP_DESK.Detailed_Decription,
  HPD_STATUS.STATUS,
  HPD_PRIORITY.priority,
  dbo.fn_adjusted_date( HPD_HELP_DESK.Submit_Date) as Submit_Date,
  HPD_HELP_DESK.Assigned_Group 
  from HPD_HELP_DESK
  left outer join hpd_status
  on HPD_HELP_DESK.Status =  hpd_status.ENUMID
  left outer join hpd_priority
  on HPD_HELP_DESK.Priority = hpd_priority.ENUMID

  WHERE
  (
   HPD_HELP_DESK.Assigned_Group   IN  ( 'NP_RNPO_srb'  )
   AND
   HPD_STATUS.STATUS  IN  ( 'Assigned','In Progress','Pending'  )
  )

  order by Submit_Date</CommandText>
      </Query>
