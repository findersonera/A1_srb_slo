  <DataSets>
    <DataSet Name="a1telekom_prd_ar">
      <Query>
        <DataSourceName>a1telekom_prd_ar</DataSourceName>
        <CommandText>SELECT srm_woi_workorder.work_order_id, 
       srm_woi_workorder.summary, 
       srm_woi_workorder.detailed_description, 
       srm_wo_status.status, 
       srm_wo_priority.priority, 
       dbo.Fn_adjusted_date(srm_woi_workorder.submit_date) AS Submit_Date, 
       srm_woi_workorder.asgrp 
FROM   srm_woi_workorder 
       LEFT OUTER JOIN srm_wo_status 
                    ON srm_woi_workorder.status = srm_wo_status.enumid 
       LEFT OUTER JOIN srm_wo_priority 
                    ON srm_woi_workorder.priority = srm_wo_priority.enumid 
WHERE  ( dbo.Fn_adjusted_date(srm_woi_workorder.submit_date) BETWEEN 
                  '01/01/2019 00:0:0' AND getdate()
         AND srm_woi_workorder.asgrp IN ( 'CFS_CSO_srb', 'CFS_MNP_srb' ) ) 
ORDER  BY submit_date </CommandText>
      </Query>
