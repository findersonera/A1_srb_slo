  <DataSets>
    <DataSet Name="a1telekom_prd_ar">
      <Query>
        <DataSourceName>a1telekom_prd_ar</DataSourceName>
        <CommandText>  SELECT
  HPD_HELP_DESK.Incident_Number ,
  HPD_STATUS.STATUS,
  HPD_PRIORITY.priority,
  HPD_HELP_DESK.Product_Categorization_Tier_1 ,
  HPD_HELP_DESK.Product_Categorization_Tier_2 ,
  HPD_HELP_DESK.Product_Categorization_Tier_3 ,
  HPD_HELP_DESK.ServiceCI,
  HPD_HELP_DESK.Assigned_Group ,
  HPD_HELP_DESK.Assignee ,
  dbo.fn_adjusted_date(  HPD_HELP_DESK.Closed_Date) as Closed_Date,
  SLM_STATUS.SLM_Status ,
  dbo.fn_adjusted_date( HPD_HELP_DESK.Submit_Date) as Submit_Date,
  HPD_HELP_DESK.Categorization_Tier_2 ,
  HPD_HELP_DESK.Detailed_Decription,
  HPD_HELP_DESK.Resolution,
  HPD_HELP_DESK.Country ,
  HPD_HELP_DESK.Description,
  HPD_HELP_DESK.HPD_CI,
  dbo.fn_adjusted_date( HPD_HELP_DESK.Last_Resolved_Date) as Last_Resolved_Date ,
  case when ( dbo.fn_adjusted_date(  HPD_HELP_DESK.Closed_Date) ) is null 
then
(
case when ( dbo.fn_adjusted_date( HPD_HELP_DESK.Re_Opened_Date) ) is null 
then
datediff(dd,( dbo.fn_adjusted_date(  HPD_HELP_DESK.Reported_Date) ),( getdate() )) 
else
datediff(dd,( dbo.fn_adjusted_date( HPD_HELP_DESK.Re_Opened_Date) ),( getdate() )) 
end
)
else datediff(dd,( dbo.fn_adjusted_date(  HPD_HELP_DESK.Reported_Date) ),( dbo.fn_adjusted_date(  HPD_HELP_DESK.Closed_Date) )) 
end  as Age_Of_Incident
,
  dbo.fn_adjusted_date( SLM_MEASUREMENT.SVTDueDate) as SVTDueDate ,
  DATEPART(WK,dbo.fn_adjusted_date( HPD_HELP_DESK.Reported_Date)) as Week_Number,
dbo.fn_adjusted_date( HPD_HELP_DESK.Reported_Date) as Reported_Date 

FROM

HPD_HELP_DESK
left outer join hpd_status
on HPD_HELP_DESK.[Status] = hpd_status.ENUMID
left outer join hpd_priority
on HPD_HELP_DESK.Priority = hpd_priority.ENUMID
left outer join SLM_STATUS
on HPD_HELP_DESK.SLM_Status = SLM_Status.ENUMID
left outer join SLM_MEASUREMENT
on hpd_help_desk.instanceid = SLM_MEASUREMENT.applicationinstanceid

WHERE
  (
   dbo.fn_adjusted_date( HPD_HELP_DESK.Reported_Date)  &gt;  '01/01/2016 00:0:0'
   AND
   HPD_HELP_DESK.Assigned_Group   IN  ( 'BS &amp; DS_Billing Mediation','BS &amp; DS_Billing Postpaid_sim','BS &amp; DS_Billing Postpaid_vsr','BS &amp; DS_Billing Prepaid','BS &amp; DS_Billing R&amp;I_vsr','BS &amp; DS_Customer Facing MNP_vsr','BS &amp; DS_Billing Mediation_sim','BS &amp; DS_CRM_sim','BS &amp; DS_Provisioning_sim','BS &amp; DS_Provisioning_vsr','BS &amp; DS_Enterprise Applications_sim','BS &amp; DS_Integration (ESB)_sim','BS &amp; DS_Customer Facing CSO_vsr','IT &amp; SP &amp; Delivery_ Payment_sim','Backend &amp; Billing_Mediation','Backend &amp; Billing_Postpaid_slo','Backend &amp; Billing_Provisioning_slo','Backend &amp; Billing_Postpaid_srb','Backend &amp; Billing_Provisioning_srb','Backend &amp; Billing_R&amp;I_srb'  )
   AND
   HPD_STATUS.STATUS  IN  ( 'Resolved','Closed','Cancelled'  )
  )


order by HPD_HELP_DESK.Reported_Date</CommandText>
      </Query>
