  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>a1telekom_prd_ar</DataSourceName>
        <CommandText>SELECT
  HPD_HELP_DESK.Incident_Number ,
  HPD_STATUS.STATUS,
  HPD_PRIORITY.priority,
  HPD_HELP_DESK.Assigned_Group ,
  HPD_HELP_DESK.Assignee ,
  dbo.fn_adjusted_date(  HPD_HELP_DESK.Closed_Date) as Closed_Date,
  SLM_STATUS.SLM_Status ,
  HPD_HELP_DESK.First_Name ,
  HPD_HELP_DESK.Last_Name ,
  dbo.fn_adjusted_date( HPD_HELP_DESK.Submit_Date) as Submit_Date,
  dbo.fn_adjusted_date(  HPD_HELP_DESK.Responded_Date) as Responded_Date,
  HPD_HELP_DESK.Description,
  dbo.fn_adjusted_date( HPD_HELP_DESK.Last_Resolved_Date)  as Last_resolved_date

  from HPD_HELP_DESK
  left outer join hpd_status
  on HPD_HELP_DESK.Status = hpd_status.ENUMID
  left outer join hpd_priority
  on hpd_help_desk.priority = hpd_priority.enumid
  left outer join SLM_STATUS
  on HPD_HELP_DESK.SLM_Status = SLM_STATUS.ENUMID

  WHERE
  (
   HPD_STATUS.STATUS  IN  ( 'Resolved','Closed'  )
   AND
   HPD_HELP_DESK.Assigned_Group   IN  ( 'IP Network_slo'  )
   AND
   dbo.fn_adjusted_date( HPD_HELP_DESK.Submit_Date)  &lt;=  getdate()
  )
</CommandText>
      </Query>
